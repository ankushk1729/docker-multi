{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "ankush1729/multi-client",
            "hostname": "client",
            "essential": "false",
            "memory":128
        },
        {
            "name": "server",
            "image": "ankush1729/multi-server",
            "hostname": "api",
            "essential": "false",
            "memory":128
        },
        {
            "name": "worker",
            "image": "ankush1729/multi-worker",
            "hostname": "worker",
            "essential": "false",
            "memory":128
        },
        {
            "name": "nginx",
            "image": "ankush1729/multi-nginx",
            "hostname": "nginx",
            "essential": "true",
            "memory":128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        }
    ]
}